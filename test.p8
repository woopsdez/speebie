pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

debug_flag = false
scene = "title"

-- variables for blink
blk = {}
blk.cnt = 0
blk.wait = 16

-- variables for time
times = {nt = time()}

-- variables player
player = {
    x = 10,
    y = 98,
    w = 2,
    h = 2,
    dir = 1,
    flip_x = false,
    move = 1,
    state = "walk",
    def = {200, 232},
    dmg = {202, 234},
    fly = {204, 206, 236, 238},
    anim = {
        sprs = def,
        curr = 1,
        cnt = 1,
        wait = 16,
    },
}

items = {}

explosion = {
    w = 16,
    h = 16,
    anim = {
        sprs = {160,162,164,166,168},
        curr = 1,
        cnt = 1,
        wait = 2,
    },
}

title_initiarized = false
game_initiarized = false

//================================//
// -- util
//================================//

function alignnumber(v)
    v = ""..v
    if #v == 1 then
        v = "0"..v
    end
    return v
end

function flip_player()
    player.dir = player.dir * -1
    player.flip_x = not player.flip_x
end

function blink(table)
    if blk.cnt % blk.wait < blk.wait/2 then 
        print(table.msg, table.x, table.y, table.color1)
    else
        print(table.msg, table.x, table.y, table.color2)
    end
    blk.cnt = blk.cnt + 1
end

function heighlight(n,x,y,col)
    for i = 1, 16 do
        pal(i, col)
    end
    spr(n,x,y+1)
    spr(n,x,y-1)
    spr(n,x+1,y)
    spr(n,x-1,y)
    pal()
    spr(n,x,y)
end

//================================//
// -- draw
//================================//
function debug(bool,v)
    if bool then
        rectfill(0,120, 128,128, 1)
        print(v, 5,121, 7)
    end
end

function draw_title()
    cls()
    spr(0,78,42,4,6)
    spr(4,22,56,7,4)

    msg_startbutton = {}
    msg_startbutton.msg = "press z to start"
    msg_startbutton.x = 22
    msg_startbutton.y = 75
    msg_startbutton.color1 = 7
    msg_startbutton.color2 = 1

    blink(msg_startbutton)
end

function title_isend()
    if btn(4) then
        sfx(5)
        print("press z to start", 22, 74, 11)
        presssec = time()
        return true
    end
    return false
end

function update_title()   
    if title_isend() then
        scene = "game"
    end
end

function animate(anim) 
    if anim.cnt >= anim.wait then
        if count(anim.sprs) > anim.curr then
            anim.curr = anim.curr + 1
        else
            anim.curr = 1
        end
        anim.cnt = 1
    else
        anim.cnt = anim.cnt + 1
    end
end

function draw_player()
    if player.state == "walk" then
        player.anim.sprs = player.def
        sfx(-1,1,6)
    elseif player.state == "cry" then
        player.anim.sprs = player.dmg
        sfx(-1,1,6)
    elseif player.state == "fly" then
        player.anim.sprs = player.fly
        sfx(6,1,1)
    end
    spr(player.anim.sprs[player.anim.curr], player.x, player.y, player.w, player.h, player.flip_x)
    animate(player.anim)
end

function update_player()
    if player.state == "walk" then
        player.x = player.x + player.move * player.dir
        if player.x >= 100 or player.x <= 10 then
            flip_player()
        end
    elseif player.state == "cry" then
        spendsec = time() - dmgsec
        if spendsec > 3 then
            player.state = "walk"
        end  
    elseif player.state == "fly" then
        player.x = player.x + 2 * player.dir
        if player.x >= 100 or player.x <= 10 then
            flip_player()
        end
    end

    if btnp(0) and btn(0) then
        flip_player()
        player.state = "walk"
    end

    if btnp(1) and btn(1) then
        sfx(2)
        player.state = "cry"
        dmgsec = time()
    end

    if btnp(2) and btn(2) then
        sfx(3)
        player.state = "fly"
    end
end
    
function spawn_item()
    if flr(rnd(49)) == 0 then
        add(items, make_item())
    end
end

function update_game()
    update_player()
    foreach(items, function(item) update_item(item) end)
    spawn_item()
end

function make_item()
    local sprs = {145, 146, 147, 148}
    local item = {
        type = flr(rnd(4)+1),
        speed = rnd(8) + 1,
        x = rnd(108) + 8,
        y = 0,
        w = 8,
        h = 8,
        state = "dropping",
        sprs = sprs,
        anim = {
            sprs = {sprs[1], 144},
            curr = 1,
            cnt = 1,
            wait = 2,
        },
    }
    return item
end

function draw_item(item)
    if item.state == "dropping" then
        heighlight(item.sprs[item.type], item.x, item.y, col)
    elseif item.state == "break" then
        item.anim.sprs[1] = item.sprs[item.type]
        heighlight(item.anim.sprs[item.anim.curr], item.x, item.y, col)
        animate(item.anim)
        spr(explosion.anim.sprs[explosion.anim.curr], item.x, 100-3, 2, 2)
        animate(explosion.anim)
    elseif item.state == "break-end" then
        heighlight(item.sprs[item.type], item.x, item.y, col)
    end
end

function update_item(item)
    if item.state == "dropping" then
        item.y = item.y + item.speed
        if item.y > 100 then
            sfx(10)
            gs = time()
            item.state = "break"
        end
    elseif item.state == "break" then
        local spendsec = time() - gs
        if spendsec > 1 then
            item.state = "break-end"
        end        
    elseif item.state == "break-end" then
        item.y = 1
        del(items, item)
    end
end

function draw_time()
    m = flr((time() - times.nt)/60)
    s = flr((time() - times.nt)%60)
    m = alignnumber(m)
    s = alignnumber(s)
    t = m..":"..s
    print(t, 100, 11, 6) 
    print(t, 99, 10, 1) 
end

function draw_score()
    print("00000000", 11, 11, 6)
    print("00000000", 10, 10, 1)
end

function draw_menu()
    draw_time()
    draw_score()
end

function draw_game()
    spendsec = time() - presssec
    if spendsec > 1 then
        cls()
        map(0,0, 0,0, 16,16)
        draw_menu()
        draw_player()
        foreach(items, function(item) draw_item(item) end)
        map(16,0, 0,0, 16,16)
    end 
end

function init_game()
    srand(flr(time()))
    music(2)
end

function init_title()
    music(-1)
end

function _update()
    if scene == "title" then
        if not title_initiarized then
            init_title()
            title_initiarized = true
        end
        update_title()
    elseif scene == "game" then
        if not game_initiarized then
            init_game()
            game_initiarized = true
        end
        update_game()
    end
end

function _draw()
    if scene == "title" then
        draw_title()
    elseif scene == "game" then
        draw_game()
    end
end

